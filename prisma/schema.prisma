
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Company {
  id               String   @id @default(uuid())
  name             String
  location         String
  industry         String
  sponsorshipScore Float    @default(0)
  createdAt        DateTime @default(now())
}